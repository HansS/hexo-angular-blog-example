[
  <% posts.forEach(function(post, index) { %>
  <% if (index > 0) { %>,<% } %>
  {
    "title": "<%= post.title %>",
    "description": "<%- sanitize(post.excerpt) %>",
    "date": "<%= post.updated.toDate().toISOString() || post.date.toDate().toISOString() %>",
    "href": "<%= encodeURI(permalink(post.permalink)) %>"
  }
  <% }) %>
]
